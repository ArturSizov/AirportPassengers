<Window x:Class="AirportPassengers.Views.AddPassengerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AirportPassengers.Views"
        mc:Ignorable="d"
        Height="340" 
        Width="240"
        FontSize="14"
        WindowStartupLocation="CenterScreen"
        x:Name="Window"
        DataContext="{Binding Source={StaticResource Locator}, Path=AddPassengerWindowViewModel, UpdateSourceTrigger=PropertyChanged}"
        Title="{Binding Title}"
        ResizeMode="NoResize">
    <Grid>
        
    </Grid>
</Window>
